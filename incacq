#! /bin/bash
export DYLD_LIBRARY_PATH=/Volumes/Untitled/installs/lib

#date=('141222' '141215' '141128' '141121' '141114' '141107' '141029' '141024' '141010' '141003' '140926' '140919' '140911' '140904' '140828' '140821' '140812' '140730' '140723' '140718' '140711' '140707' '140625' '140613' '140606' '140528' '140515' '140502' '140424' '140416' '140409' '140314' '140205' '140129' '140122' '140117' '140106' '141222' '141215' '141128' '141121' '141114' '141107' '141029' '141024');

date=('151221' '151214' '151209' '151127' '151118' '151111' '151105' '151027' '151019' '151008' '150929' '150918' '150910' '150902' '150827' '150820' '150813' '150806' '150730' '150723' '150708' '150701' '150625' '150620' '150608' '150522' '150505' '150429' '150421' '150414' '150408' '150401' '150327' '150320' '150312' '150305' '150226' '150216' '150111' '151221' '151214' '151209' '151127' '151118' '151111' '151105' '151027');



val=-100000000;
for d in "${date[@]}"
do
     if [[ "$1" == "$d" ]]; then
         val=0;
     fi

     if [[ $val -gt 0 ]]; then
          if [[ $val -gt 8 ]]; then
              break;
          fi

          for i in `seq 1 10`;
          do
               ./build/consec $1 $d 14
          done
     fi
     let "val += 1";
done





