#! /bin/bash
export DYLD_LIBRARY_PATH=/Volumes/Untitled/installs/lib

date=('141222' '141215' '141128' '141121' '141114' '141107' '141029' '141024' '141010' '141003' '140926' '140919' '140911' '140904' '140828' '140821' '140812' '140730' '140723' '140718' '140711' '140707' '140625' '140613' '140606' '140528' '140515' '140502' '140424' '140416' '140409' '140314' '140205' '140129' '140122' '140117' '140106' '141222' '141215' '141128' '141121' '141114' '141107' '141029' '141024');

val=-100000000;
for d in "${date[@]}"
do
     if [[ "$1" == "$d" ]]; then
         val=0;
     fi

     if [[ $val -gt 0 ]]; then
          if [[ $val -gt 8 ]]; then
              break;
          fi

          for i in `seq 1 10`;
          do
               ./build/consec $1 $d 14
          done
     fi
     let "val += 1";
done





